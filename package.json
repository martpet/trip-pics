{
  "name": "trippics",
  "version": "0.0.0",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "start": "vite serve frontend",
    "test": "npm run check-types && npm run lint && npm run test:all",
    "check-types": "npm run check-types:frontend && npm run check-types:backend",
    "check-types:frontend": "tsc --project frontend/tsconfig.json",
    "check-types:backend": "tsc --project backend/tsconfig.json",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "eslint frontend",
    "lint:backend": "eslint backend",
    "test:all": "npm run test:frontend npm run test:backend",
    "test:frontend": "",
    "test:backend": "jest --config=backend/jest.config.ts",
    "build": "npm run build:frontend && npm run synth",
    "build:frontend": "vite build frontend",
    "synth": "cd backend && npx cdk synth",
    "prepare": "husky install"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "aws-cdk-lib": "^2.27.0",
    "constructs": "^10.1.32",
    "dotenv-flow": "^3.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/dotenv-flow": "^3.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.0",
    "aws-cdk": "^2.27.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.9.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-use-nesting": "^3.0.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-plugin-rewrite-all": "^0.1.2",
    "vite-plugin-svgr": "^2.1.0"
  }
}
