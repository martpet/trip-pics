{
  "name": "trippics",
  "version": "0.0.0",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "start": "vite serve frontend",
    "test": "npm run check && npm run lint && npm run test:backend",
    "check": "npm run check:frontend && npm run check:backend",
    "check:frontend": "tsc --project frontend/tsconfig.json",
    "check:backend": "tsc --project backend/tsconfig.json",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "eslint frontend",
    "lint:backend": "eslint backend",
    "test:backend": "jest --config=backend/jest.config.ts",
    "build": "vite build frontend",
    "synth": "cd backend && npx cdk synth --context envName=Personal",
    "deploy": "npm t & npm run build && cd backend && npx cdk deploy --context envName=Personal --outputs-file ../frontend/public/cdk-output.json",
    "destroy": "cd backend && npx cdk destroy",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.118.0",
    "@aws-sdk/client-dynamodb": "^3.131.0",
    "@aws-sdk/client-sns": "^3.118.1",
    "@aws-sdk/client-ssm": "^3.128.0",
    "@aws-sdk/client-sts": "^3.128.0",
    "app-root-path": "^3.0.0",
    "aws-cdk-lib": "2.32.0",
    "constructs": "^10.1.46",
    "dotenv-flow": "^3.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/aws-lambda": "^8.10.101",
    "@types/dotenv-flow": "^3.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.0.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^2.0.0",
    "aws-cdk": "2.32.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.9.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-use-nesting": "^3.0.0",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3",
    "vite": "^3.0.0",
    "vite-plugin-rewrite-all": "^0.1.2",
    "vite-plugin-svgr": "^2.1.0"
  }
}
